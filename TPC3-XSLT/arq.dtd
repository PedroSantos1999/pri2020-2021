<?xml version="1.0" encoding="UTF-8"?>
<!ELEMENT ARQSITS (ARQELEM+)>
<!ELEMENT ARQELEM (TIPO|IDENT|IMAGEM|DESCRI|LUGAR|FREGUE|CONCEL|CODADM|LATITU|LONGIT|ALTITU|ACESSO|QUADRO|DESARQ|INTERP|DEPOSI|BIBLIO|AUTOR|DATA|CRONO|TRAARQ|INTERE)>

